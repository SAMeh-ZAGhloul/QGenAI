# Database
DATABASE_URL=postgresql://postgres:postgres@localhost/knowledge_navigator

# Security
SECRET_KEY=your-secret-key-for-jwt

# Vector Database
VECTOR_DB_PATH=./vector_db

# Document Storage
UPLOAD_FOLDER=./uploads

# LLM Configuration
OPENAI_API_KEY=your-openai-api-key
GOOGLE_API_KEY=your-google-api-key
EMBEDDING_MODEL=text-embedding-3-small
# LLM Provider: openai, google, ollama
LLM_PROVIDER=google
# OpenAI model: gpt-3.5-turbo, gpt-4, etc.
LLM_MODEL=gpt-3.5-turbo
# Google model: gemini-pro, gemini-1.5-pro, etc.
GOOGLE_MODEL=gemini-1.5-pro
# Ollama model: llama2, mistral, etc.
OLLAMA_MODEL=llama2
OLLAMA_URL=http://localhost:11434
